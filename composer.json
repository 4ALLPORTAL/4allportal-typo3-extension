{
  "name": "fourallportal/fourallportal-typo3-extension",
  "type": "typo3-cms-extension",
  "description": "This TYPO3 extension enables the 4allportal-typo3-connector to send files from 4ALLPORTAL to TYPO3.",
  "authors": [],
  "version": "1.0.1",
  "license": "MIT",
  "require": {
    "typo3/cms-core": "^13.0",
    "nng/nnrestapi": "^13.0 || dev-13*",
    "nng/nnhelpers": "^13.0 || dev-13*"
  },
  "autoload": {
    "psr-4": {
      "Fourallportal\\Fourallportalext\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "fourallportal_typo3_extension",
      "skip-ter-packagist-import": false
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
